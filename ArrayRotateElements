#Rotate array elements one by one
def ArrayRotateElements(a,d):
    n=len(a)
    print(n)
    for i in range (d):

        temp= a[0]

        for i in range (n-1):

            a[i]= a[i+1]

        a[n-1]= temp

    return a 

def main():

    print (ArrayRotateElements([3,4,5,6,7],2))

if (__name__=="__main__"):
    main()
