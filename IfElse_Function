#This program helps on 3 things, take input from user, define a function, if else clause and how to run main function

def ifelse():
    num= int(input("Enter a num"))
    if (num<100):
        return ("Less than 100")
    else:
        return ("Greater than 100")

def main():
    print("Entered main")

    val=ifelse()

    print(val)

if __name__=="__main__": 
    main()

